{
  "presets": [
    "next/babel",
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry"
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    "styled-components",
    [
      "module-resolver",
      {
        "root": ["./"]
      }
    ]
  ]
}
